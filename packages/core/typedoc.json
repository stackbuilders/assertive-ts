{
  "$schema": "https://typedoc.org/schema.json",
  "cleanOutputDir": true,
  "entryPoints": ["src/index.ts"],
  "entryPointStrategy": "expand",
  "gitRevision": "main",
  "githubPages": false,
  "hideGenerator": true,
  "includeVersion": false,
  "intentionallyNotExported": ["NoThrow"],
  "mergeModulesMergeMode": "project",
  "mergeModulesRenameDefaults": true,
  "name": "Assertive.ts - Core API",
  "out": "../../docs/core/build",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-plugin-merge-modules"
  ],
  "readme": "none",
  "visibilityFilters": {
    "@alpha": false,
    "@beta": false,
    "external": false,
    "inherited": true,
    "private": false,
    "protected": false
  }
}
